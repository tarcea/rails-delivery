<%= simple_form_for @resource do |f| %>
  <%= f.input :url %>
  <%= f.input :description %>
  <%= f.select :discipline, ["Ruby on Rails", "PostgreSQL", "Mongo DB", "Java script", "NodeJS", "React", "Heroku", "Bootstrap",
            "HTML", "CSS", "Git", "Github", "Slack", "Trello",
            "Figma"], class: 'form-control' %>
  <%= f.button :submit, class: 'btn' %>
<% end %>
